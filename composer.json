{
    "name"       : "sabre/katana",
    "type"       : "project",
    "description": "A CalDAV, CardDAV and WebDAV ready-to-use server on top of sabre/dav",
    "homepage"   : "http://sabre.io/katana/",
    "license"    : "BSD-3-Clause",
    "require"    : {
        "sabre/dav"     : "~2.1",
        "hoa/console"   : "~2.0",
        "hoa/dispatcher": "~0.0",
        "hoa/router"    : "dev-dav as 2.15.02.17",
        "php"           : ">=5.4.7"
    },
    "authors": [
        {
            "name": "fruux"
        }
    ],
    "support" : {
        "forum" : "http://groups.google.com/group/sabredav-discuss",
        "source" : "https://github.com/fruux/sabre-katana/"
    },
    "require-dev": {
        "atoum/atoum"            : "~1.0",
        "atoum/praspel-extension": "~0.1"
    },
    "autoload": {
        "psr-4": {
            "Sabre\\Katana\\": "lib/"
        }
    },
    "bin": ["bin/katana"],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/Hywan/Router"
        }
    ],
    "config" : {
        "bin-dir" : "bin/"
    }
}
