{
    "name"       : "sabre/katana",
    "type"       : "project",
    "description": "A CalDAV, CardDAV and WebDAV ready-to-use server on top of sabre/dav",
    "require"    : {
        "sabre/dav"     : "~2.1.0",
        "hoa/console"   : "~2.0",
        "hoa/dispatcher": "~0.0",
        "hoa/router"    : "dev-dav as 2.15.02.17"
    },
    "require-dev": {
        "atoum/atoum"            : "~1.0",
        "atoum/praspel-extension": "~0.1"
    },
    "license": "proprietary",
    "authors": [
        {
            "name": "fruux"
        }
    ],
    "autoload": {
        "psr-4": {
            "Sabre\\Katana\\": "lib/"
        }
    },
    "bin": ["bin/katana"],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/Hywan/Router"
        }
    ]
}
