    "breadcrumb",
    "button",
    "checkbox",
    "dimmer",
    "dropdown",
    "form",
    "grid",
    "header",
    "icon",
    "image",
    "input",
    "item",
    "label",
    "list",
    "loader",
    "menu",
    "message",
    "modal",
    "progress",
    "reset",
    "segment",
    "site",
    "table",
    "transition"
  ],
  "version": "1.12.3"
}